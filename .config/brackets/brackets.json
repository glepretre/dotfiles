        },
        {
            "trigger": "cd",
            "scope": "javascript",
            "description": "console.dir",
            "tag": "VanillaJS",
            "tagHide": true,
            "text": "console.dir(${1:obj});${2}",
            "source": "system"
        },
        {
            "trigger": "cll",
            "scope": "javascript",
            "description": "console.log",
            "tag": "VanillaJS",
            "tagHide": true,
            "text": "console.log('${1:obj}:', ${1:obj});${2}",
            "source": "system"
        },
        {
            "trigger": "qs",
