    ],
    "fileFilters": [
        {
            "name": "fatFolders",
            "patterns": [
                ".latest-stash",
                "bower_components",
                "build",
                "db",
                "deploy",
                "dist",
                "lib",
                "libraries",
                "log",
                "node_modules",
                "tools",
                "vendor"
            ]
        },
        {
            "name": "fatFolders+tests",
            "patterns": [
                "*_test.js",
                ".latest-stash",
                "bower",
                "bower_components",
                "build",
                "db",
                "deploy",
                "dist",
                "e2e",
                "lib",
                "libraries",
                "log",
                "node_modules",
                "proto",
                "spec",
                "tools",
                "vendor"
            ]
        }
    ],
    "findInFiles.instantSearch": false,
    "findInFiles.nodeSearch": false,
    "fonts.fontSize": "12px",
    "fonts.fontFamily": "'SourceCodePro-Medium', ＭＳ ゴシック, 'MS Gothic', monospace",
    "guidelines.columns": "80",
    "guidelines.columnColor": "#B0B4B9",
    "guidelines.enabled": true,
    "highlightMatches": true,
    "insertHintOnTab": true,
    "language": {
        "javascript": {
            "linting.prefer": [
                "JSHint",
                "JSCS"
            ],
            "linting.usePreferredOnly": true
        }
    },
    "language.fileExtensions": {
        "": "bash"
    },
    "linting.enabled": true,
    "linting.collapsed": true,
    "markdown-preview.useGFM": true,
    "markdown-preview.theme": "dark",
    "mikaeljorhult.bracketsTodo.enabled": false,
    "mikaeljorhult.bracketsTodo.userSettings": {
        "regex": {
            "prefix": "(?:\\/\\*|\\/\\/|#|\\*) *@?(",
            "suffix": "):? *(.*?) ?(?=\\*/|\\n|$)"
        },
        "tags": [
            "TODO",
            "NOTE",
            "FIX ?ME",
            "MEMO"
        ],
        "case": true,
        "search": {
            "scope": "project",
            "excludeFolders": [
                "bower_components",
                "build",
                "db",
                "deploy",
                "dist",
                "lib",
                "libraries",
                "log",
                "node_modules",
                "tools",
                "vendor"
            ],
            "excludeFiles": [
                ".latest-stash",
                "application.css",
                "base.css"
            ]
        },
        "sort": {
            "done": true
        }
    },
    "mikaeljorhult.bracketsTodo.visibleFiles": [],
    "mikaeljorhult.bracketsTodo.visibleTags": {
        "todo": {
            "tag": "todo",
            "name": "TODO",
            "count": 0,
            "visible": true
        },
        "note": {
            "tag": "note",
            "name": "NOTE",
            "count": 0,
            "visible": true
        },
        "fixme": {
            "tag": "fixme",
            "name": "FIXME",
            "count": 0,
            "visible": true
        }
    },
    "path": {
        "**/*/Makefile": {
            "brackets-wsSanitizer.enabled": false,
            "useTabChar": true
        }
    },
    "smartIndent": true,
    "spaceUnits": 2,
    "styleActiveLine": true,
    "tabSize": 2,
    "useTabChar": false,
    "themes.theme": "rain.monokai-dark-soda",
    "vimderbar.enabled": false
}
