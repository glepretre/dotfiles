# Override git user.email in personal GitHub projects
# Paste it on top of .git/config in these projects

[user]
  email = glepret.pro@gmail.com
  name = Gilles Lepretre

[remote "origin"]
  # Must be https not ssh (git@github.com:...)
  url = https://github.com/glepretre/REPOSITORY.git
  fetch = +refs/heads/*:refs/remotes/origin/*

#
# Or use this command to push
# GIT_SSH_COMMAND="ssh -i /home/gilles/.ssh/GITHUB_SSH_KEY" git push
# then use token
#
